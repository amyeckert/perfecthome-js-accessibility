  "bugs": {
    "url": "https://github.com/amyeckert/perfecthome-js/issues"
  },
  "homepage": "https://github.com/amyeckert/perfecthome-js#readme",
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.2.2",
    "browserify": "^13.3.0",
    "concurrently": "^9.2.1",
    "del": "^8.0.1",
    "gulp": "^5.0.1",
    "gulp-autoprefixer": "^10.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-htmlmin": "^3.0.0",

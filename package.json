    "gulp-minify-css": "^1.2.4",
    "gulp-notify": "^5.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "merge-stream": "^2.0.0",
    "sass": "^1.80.0",
    "vinyl-source-stream": "^1.1.0"
